class Solution {
public:
    bool isValid(string str) {
        unordered_map<char,int>symbols = {{'(',1},{'[',2},{'{',3},{')',-1},{']',-2},{'}',-3}};
        stack<char>s;
        for(int i = 0; i < str.length(); i++){
            if(s.empty()){
                s.push(str[i]);
            }else if(symbols[str[i]] == -1 || symbols[str[i]] == -2 || symbols[str[i]] == -3){
                int element_top = symbols[s.top()];
                s.pop();
                if(element_top + symbols[str[i]] != 0){
                    return false;
                }else{
                    continue;
                }
            }else{
                s.push(str[i]);
            }
        }

        if(s.empty()){
            return true;
        }else{
            return false;
        }

        
    }
};
