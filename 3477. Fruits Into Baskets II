class Solution {
public:
    int numOfUnplacedFruits(vector<int>& fruits, vector<int>& baskets) {
        int n = fruits.size();
        int m = baskets.size();
        int count = 0;

        for(int i = 0; i < n; i++){
            int find = 0;
            for(int j = 0; j < m; j++){
                  if(fruits[i] <= baskets[j]){
                    baskets[j] = 0;
                    find = 0;
                    break;
                }else if(fruits[i] > baskets[j]){
                    find = 1;
                }
            }
            if(find > 0){
                count++;
            }
        }

        return count;
        
    }
};
