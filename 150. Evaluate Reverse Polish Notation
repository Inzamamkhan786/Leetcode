class Solution {
public:
    int evalRPN(vector<string>& tokens) {
        int sum = 0;
        stack<int>s;
        for(int i = 0; i < tokens.size(); i++){
            if(tokens[i] == "+"){
                int element1 = s.top();
                s.pop();
                int element2 = s.top();
                s.pop();
                sum = sum + (element2 + element1);
                s.push(sum);
                sum = 0;
            }else if(tokens[i] == "-"){
                int element1 = s.top();
                s.pop();
                int element2 = s.top();
                s.pop();
                sum = sum + (element2 - element1);
                s.push(sum);
                sum = 0;
            }else if(tokens[i] == "*"){
                int element1 = s.top();
                s.pop();
                int element2 = s.top();
                s.pop();
                sum = sum + (element2 * element1);
                s.push(sum);
                sum = 0;
            }else if(tokens[i] == "/"){
                int element1 = s.top();
                s.pop();
                int element2 = s.top();
                s.pop();
                sum = sum + (element2 / element1);
                s.push(sum);
                sum = 0;
            }else {
                int num = stoi(tokens[i]);
                s.push(num);
            }
        }

        int calculation = s.top();
        return calculation;
        
    }
};
